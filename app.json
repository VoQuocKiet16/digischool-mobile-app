{
  "expo": {
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-digi-icon.png",
    "scheme": "digischool",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/app-digi-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-digi-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gdevx.digischool"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-digi-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b17d889f-c31f-4576-a6d8-24a04087a724"
      }
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ]
  }
}
